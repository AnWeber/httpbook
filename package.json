{
  "name": "httpbook",
  "displayName": "httpBook - Rest Client",
  "description": "Quickly and easily send REST, SOAP, and GraphQL requests directly within Visual Studio Code",
  "version": "0.2.0",
  "publisher": "anweber",
  "homepage": "https://github.com/AnWeber/httpbook",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnWeber/httpbook"
  },
  "bugs": {
    "url": "https://github.com/AnWeber/httpbook/issues"
  },
  "qna": "https://github.com/AnWeber/httpbook/discussions",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Notebooks"
  ],
  "keywords": [
    "HTTP",
    "REST",
    "GraphQL",
    "Intellij Http Client",
    "Postman",
    "Soap",
    "ci",
    "api",
    "rest-client"
  ],
  "icon": "icon.png",
  "preview": true,
  "enableProposedApi": true,
  "activationEvents": [
    "onNotebook:http",
    "onLanguage:http"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "notebookProvider": [
      {
        "viewType": "http",
        "displayName": "HttpBook",
        "selector": [
          {
            "filenamePattern": "*.http"
          }
        ]
      }
    ],
    "notebookOutputRenderer": [
      {
        "id": "httpbook",
        "entrypoint": "./dist/client.js",
        "displayName": "httpbook",
        "mimeTypes": [
          "x-application/httpbook-testresults",
          "x-application/httpbook-rfc7230"
        ]
      }
    ],
    "configuration": {
      "type": "object",
      "title": "httpbook",
      "properties": {
        "httpbook.responseMimes": {
          "type": "array",
          "default": [
            "x-application/httpbook-rfc7230",
            "x-application/httpbook-mime",
            "x-application/httpbook-contenttype",
            "x-application/httpbook-markdown"
          ],
          "scope": "resource",
          "description": "list of mimes for response notebookOutputRenderer"
        },
        "httpbook.testMimes": {
          "type": "array",
          "default": [
            "x-application/httpbook-testresults",
            "x-application/httpbook-testresults-markdown"
          ],
          "scope": "resource",
          "description": "list of mimes for test notebookOutputRenderer"
        }
      }
    }
  },
  "extensionDependencies": [
    "anweber.vscode-httpyac"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production --config ./webpack.config.js",
    "watch": "webpack --mode development --watch --config ./webpack.config.js",
    "lockfile-lint": "lockfile-lint --path package-lock.json --validate-https --allowed-hosts npm --validate-checksum --validate-integrity",
    "eslint": "eslint src --ext ts",
    "lint": "npm run eslint && npm run lockfile-lint",
    "precommit": "npm run lint",
    "update-api": "npx vscode-dts dev"
  },
  "devDependencies": {
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/vscode": "^1.55.0",
    "@types/vscode-notebook-renderer": "^1.57.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "css-loader": "^5.2.4",
    "eslint": "^7.25.0",
    "eslint-plugin-node": "^11.1.0",
    "fork-ts-checker-webpack-plugin": "^6.2.6",
    "httpyac": "^2.7.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "lockfile-lint": "^4.6.2",
    "style-loader": "^2.0.0",
    "svg-sprite-loader": "^6.0.6",
    "thread-loader": "^3.0.3",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4",
    "vscode-dts": "^0.3.1",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "preact": "^10.5.13",
    "vscode-codicons": "^0.0.16",
    "vscode-notebook-error-overlay": "^1.0.1"
  }
}
