{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2019",
    "lib": [
      "ES2019"
    ],
    "types": ["node"],
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
	  "noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": true,
		"noImplicitThis": true,

  }
}
